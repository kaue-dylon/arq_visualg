algoritmo "semnome"
var
numero, resultado: real
inicio
escreval("Insira um número qualquer aqui:")
leia(numero)
resultado <- numero mod 2

se(resultado=1)entao
   escreval("Seu número é ímpar")
   se(resultado=0)entao
      escreval("Seu número é par")
   fimse
fimse



// Seção de Comandos
fimalgoritmo
